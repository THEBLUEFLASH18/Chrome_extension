{
    "manifest_version": 3,
    "name": "Job Linker",
    "version": "1.0",
    "description": "Save job links and download them as an Excel file.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "logo.png",
            "48": "logo.png",
            "128": "logo.png"
        }
    }
}